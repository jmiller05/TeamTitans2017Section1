<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="inventoryView" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="columnName" text="Name">
            <cellValueFactory>
                <PropertyValueFactory property="itemName"/>
            </cellValueFactory>
          </TableColumn>
          <TableColumn fx:id="columnDescription" text="Description">
            <cellValueFactory>
                <PropertyValueFactory property="itemDescription"/>
            </cellValueFactory>
          </TableColumn>
        </columns>
      </TableView>
   </children>
</VBox>

